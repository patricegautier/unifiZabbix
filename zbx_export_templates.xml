                    <key>5g_interference</key>
                    <units>%</units>
                    <params>(last(5g_interference_raw)&gt;0)*last(5g_interference_raw)</params>
                    <applications>
                        <application>
                            <name>Radio</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>5G Channel Interference Raw</name>
