    <templates>
        <template>
            <template>- Unifi Base -</template>
            <name>- Unifi Base -</name>
            <description>Abstract base template for all unifi devices supporting mca-dump.</description>
            <groups>
                <group>
                    <name>Patrice</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Host</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>mca-dump</name>
                    <type>SSH</type>
                    <key>ssh.run[mca-dump]</key>
                    <delay>{$UNIFI_CHECK_FREQUENCY}</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <params>mca-dump | sed 's/^[ \t]*//'</params>
                    <authtype>PUBLIC_KEY</authtype>
                    <username>{$UNIFI_USER}</username>
                    <publickey>{$UNIFI_PUB_KEY}</publickey>
                    <privatekey>{$UNIFI_PRIV_KEY}</privatekey>
                    <applications>
                        <application>
                            <name>Host</name>
                        </application>
                    </applications>
                </item>
            </items>
        </template>
        <template>
            <template>- Unifi Host -</template>
            <name>- Unifi Host -</name>
            <description>Abstract base template to support all Unifi devices and basic host level info.</description>
            <templates>
                <template>
                    <name>- Unifi Base -</name>
                </template>
            </templates>
            <groups>
                <group>
