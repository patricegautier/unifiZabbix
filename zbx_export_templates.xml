        <template>
            <template>Unifi AP</template>
            <name>Unifi AP</name>
            <description>uses mca-dump via ssh to get AP info.&#13;
&#13;
Assign this template to all APs in your site</description>
            <templates>
                <template>
                    <name>- Unifi Host -</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>Patrice</name>
                </group>
                <group>
                    <name>Unifi</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Radio</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>2G Channel Interference</name>
                    <type>CALCULATED</type>
                    <key>2g_interference</key>
                    <units>%</units>
                    <params>(last(2g_interference_raw)&gt;0)*last(2g_interference_raw)</params>
                    <applications>
                        <application>
                            <name>Radio</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>2G Channel Interference raw</name>
                    <type>CALCULATED</type>
                    <key>2g_interference_raw</key>
                    <units>%</units>
                    <params>last(cu_total_2g)- last(cu_self_tx_2g) - last(cu_self_rx_2g)</params>
                    <applications>
                        <application>
                            <name>Radio</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{avg({$UNIFI_ALERT_PERIOD})}&gt;30</expression>
                            <name>{HOST.NAME} 2G Interference &gt; 30%</name>
                            <priority>AVERAGE</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>2G Channel Interference Smooth</name>
                    <type>CALCULATED</type>
